{
  "presets": ["next/babel"],
  "plugins": [
    "inline-react-svg",
    [
      "module-resolver",
      {
        "alias": {
          "~": "./src",
          "~assets": "./public",
          "~components": "./src/components",
          "~containers": "./src/containers",
          "~pages": "./src/pages",
          "~styles": "./src/styles",
          "~tests": "./src/tests",
          "~types": "./src/types",
          "~utils": "./src/utils"
        }
      }
    ],
    "lodash",
    [
      "styled-components",
      {
        "ssr": true
      }
    ]
  ]
}
